{
  "name": "@map3xyz/assets-helper",
  "version": "1.0.19",
  "description": "CLI for maintaining the assets repo.",
  "author": "pellicceama",
  "keywords": [
    "typescript",
    "cli",
    "starter"
  ],
  "license": "MIT",
  "main": "./dist/cli.js",
  "types": "/dist/cli.d.ts",
  "bin": "dist/cli.js",
  "files": [
    "dist/**/*",
    "!dist/**/*.test.js"
  ],
  "scripts": {
    "cli": "yarn ts-node ./src/cli.ts",
    "clean": "rimraf ./dist/ ./exec/",
    "build": "yarn clean && tsc",
    "preparePublish": "yarn build && npm version patch",
    "publish": "npm publish --access public",
    "test": "yarn build && ava --verbose",
    "bundle": "yarn build && pkg . --out-dir ./exec/"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "ava": "^4.3.0",
    "pkg": "^5.7.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "commander": "^9.3.0"
  },
  "packageManager": "yarn@3.2.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/map3xyz/assets-helper.git"
  },
  "bugs": {
    "url": "https://github.com/map3xyz/assets-helper/issues"
  },
  "homepage": "https://github.com/map3xyz/assets-helper#readme"
}

{
  "name": "assets-helper",
  "private": true,
  "version": "1.0.0",
  "description": "CLI for maintaining the assets repo.",
  "author": "pellicceama",
  "keywords": [
    "typescript",
    "cli",
    "starter"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "bin": "./dist/cli.js",
  "files": [
    "dist/**/*",
    "!dist/**/*.test.js"
  ],
  "scripts": {
    "dev": "yarn ts-node ./src/cli.ts",
    "clean": "rimraf ./dist/ ./exec/",
    "build": "yarn clean && tsc",
    "test": "yarn build && ava --verbose",
    "bundle": "yarn build && pkg . --out-dir ./exec/"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "ava": "^4.3.0",
    "pkg": "^5.7.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "commander": "^9.3.0"
  },
  "packageManager": "yarn@3.2.1"
}
